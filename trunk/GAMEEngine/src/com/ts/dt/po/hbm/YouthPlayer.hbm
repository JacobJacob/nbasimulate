<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.ts.dt.po">
    <class name="YouthPlayer" table="youth_player" lazy="true">
        <id
            name="id"
            type="java.lang.Long"
            column="id"
        >
            <generator class="native"/>
        </id>
        <property
           name="no"
           column="no"
           type="java.lang.String"
        />
        <property
           name="name"
           column="name"
           type="java.lang.String"
        />
        <property
           name="teamId"
           column="team_id"
           type="java.lang.Long"
        />
        <property
           name="playerNo"
           column="player_no"
           type="java.lang.Long"
        />
        <property
           name="position"
           column="position"
           type="java.lang.String"
        />
        <property
           name="stature"
           column="stature"
           type="java.lang.Long"
        />
        <property
           name="avoirdupois"
           column="avoirdupois"
           type="java.lang.Long"
        />
        <property
           name="power"
           column="power"
           type="java.lang.Integer"
        />
        <property
           name="matchPower"
           column="match_power"
           type="java.lang.Integer"
        />
        <property
           name="ability"
           column="ability"
           type="java.lang.Double"
        />
        <property
           name="shooting"
           column="shooting"
           type="java.lang.Float"
        />
        <property
           name="speed"
           column="speed"
           type="java.lang.Float"
        />
        <property
           name="strength"
           column="strength"
           type="java.lang.Float"
        />
        <property
           name="bounce"
           column="bounce"
           type="java.lang.Float"
        />
        <property
           name="stamina"
           column="stamina"
           type="java.lang.Float"
        />
        <property
           name="trisection"
           column="trisection"
           type="java.lang.Float"
        />
        <property
           name="dribble"
           column="dribble"
           type="java.lang.Float"
        />
        <property
           name="pass"
           column="pass"
           type="java.lang.Float"
        />
        <property
           name="backboard"
           column="backboard"
           type="java.lang.Float"
        />
        <property
           name="steal"
           column="steal"
           type="java.lang.Float"
        />
        <property
           name="blocked"
           column="blocked"
           type="java.lang.Float"
        />
        <property
           name="defencons"
           column="defencons"
           type="java.lang.Float"
        />
        <property
           name="offencons"
           column="offencons"
           type="java.lang.Float"
        />
        <property
           name="buildupcons"
           column="buildupcons"
           type="java.lang.Float"
        />
        <property
           name="leadcons"
           column="leadcons"
           type="java.lang.Float"
        />
        <property
           name="backbone"
           column="backbone"
           type="java.lang.Float"
        />
</class>
</hibernate-mapping>